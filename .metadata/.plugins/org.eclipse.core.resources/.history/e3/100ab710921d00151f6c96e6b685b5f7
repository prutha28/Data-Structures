package circular.linked.lists;

public class CircularLinkedList implements ICircularLinkedList {

	class Node{
		private int data ;
		private Node next ;

		public Node(){
			this.data  = -1;
			this.next = null ;
		}

		public Node( int data ){
			this.data  = data ;
			this.next = null ;
		}
	}


	Node entry ;

	//	Empty Linked list.
	public CircularLinkedList(){
		this.entry = null ;
	}

