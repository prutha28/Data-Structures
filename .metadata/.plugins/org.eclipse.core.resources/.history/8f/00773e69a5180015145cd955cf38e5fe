package sorting.merge;

import java.util.Scanner;

public class MergeSort_Cormen_Fashion_Test {

	public static void main(String[] args) {

		Scanner input = new Scanner( System.in ) ;
		System.out.println("\nHow many elements ?");
		int n = input.nextInt() ;
		
		int A[] = null ;
		for (int i = 0; i < n; i++) {
			A[ i ] = input.nextInt() ;
		}
		
	}

}
